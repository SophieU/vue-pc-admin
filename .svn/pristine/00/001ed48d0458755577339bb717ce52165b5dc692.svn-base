.w300{
  width: 300px;
}
.w400{
  width: 400px;
}
.type-lists{
  max-height: 200px;
  overflow-y: auto;
  li{
    cursor: pointer;
    padding: 5px;
    text-align: center;
    white-space: nowrap;
    &:hover{
      background: #039dff;
      color: #fff;
    }
    &.active{
      background: #039dff;
      color: #fff;
    }
  }
}

.storage-detail{
  .native-input{
    border: 1px solid #e8eaec;
    border-radius: 4px;
    padding: 4px 10px;
    &:read-only{
      background-color: #f8f8f9;
    }
  }
}

.new-inventory{
  .inventory-step{
    text-align: center;
    span,a{
      display: inline-block;
      padding: 5px 15px 6px;
      vertical-align: top;
    }
    .text-blue{
      font-size: 14px;
    }
    .step{
      width: 34px;
      height: 34px;
      padding: 0;
      line-height: 34px;
      border: 1px solid #039dff;
      color: #fff;
      background: #039dff;
      border-radius: 100%;
    }
  }

}
