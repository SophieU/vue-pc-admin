 <Upload :action=\"uploadLink\"\n                :with-credentials=\"true\"\n                      :on-success=\"uploadSuccess\"\n              >\n                <Button class=\"step-target\">上传数据</Button>\n              </Upload>\n              <span>盘点并上传盘点数据</span>\n            </div>\n          </div>\n        </Col>\n        <Col span=\"8\">\n          <div class=\"inventory-step\">\n            <span  class=\"step\">3</span>\n            <div class=\"step-content\">\n              <Button style=\"margin-bottom: 4px;\" @click=\"sureInvent\" class=\"step-target\" >确认完成</Button>\n              <span>确认盘点</span>\n            </div>\n          </div>\n        </Col>\n      </Row>\n    </Card>\n    <Card v-if=\"uploadExcel\">\n      <p slot=\"title\">盘点预览</p>\n      <div class=\"table-wrapper\">\n        <table class=\"native-table mb-15\">\n          <thead>\n          <tr>\n            <th>组织名称</th>\n            <th>单号</th>\n            <th>创建时间</th>\n            <th>盘点盈亏</th>\n            <th>盈亏成本</th>\n            <th>操作人</th>\n          </tr>\n          </thead>\n          <tbody>\n          <tr>\n            <td>{{uploadExcel.departmentName}}</td>\n            <td>{{uploadExcel.orderSn}}</td>\n            <td>{{uploadExcel.createTime}}</td>\n            <td>{{uploadExcel.diffNum}}</td>\n            <td>{{uploadExcel.amount}}</td>\n            <td>{{uploadExcel.creator}}</td>\n          </tr>\n          </tbody>\n        </table>\n        <table class=\"native-table\">\n          <thead>\n          <tr>\n            <th>辅材名称</th>\n            <th>型号</th>\n            <th>单位</th>\n            <th>成本价</th>\n            <th>系统数量</th>\n            <th>盘点数</th>\n            <th>盈亏数</th>\n            <th>盈亏成本</th>\n          </tr>\n          </thead>\n          <tbody>\n          <tr v-for=\"item in uploadExcel.detailList\">\n            <td>{{item.materialName}}</td>\n            <td>{{item.materialSpec}}</td>\n            <td>{{item.unit}}</td>\n            <td>{{item.price}}</td>\n            <td>{{item.num}}</td>\n            <td>{{item.checkedNum}}</td>\n            <td>{{item.diffNum}}</td>\n            <td>{{item.amount}}</td>\n          </tr>\n          </tbody>\n        </table>\n      </div>\n    </Card>\n  </div>\n</template>\n\n<script>\n    export default {\n        name: \"inventory-new\",\n      computed:{\n          downLink(){\n            let baseUrl = localStorage.getItem('baseURL');\n            return baseUrl+'/repair/material/check/order/export';\n          },\n          uploadLink(){\n            let baseUrl = localStorage.getItem('baseURL');\n            return baseUrl+'/repair/material/check/order/import';\n          }\n      },\n      data(){\n          return {\n            departmentId:'',\n            uploadExcel:null,\n            sureId:null,\n          }\n      },\n      methods:{\n        uploadSuccess(res, file, fileList){\n            if(res.code===0){\n              this.uploadExcel=res.data;\n              this.sureId=res.data.id;\n            }else{\n              this.$Message.error(res.msg);\n            }\n        },\n        sureInvent(){\n            let id = this.sureId;\n            if(!id){\n              this.$Message.warning('请先上传盘点数据');\n            }else{\n              this.$http.post(`/repair/material/check/order/verify?id=${id}`)\n                .then(res=>{\n                  if(res.data.code===0){\n                    this.$Message.success('盘点成功');\n                    this.$router.push({name:'inventory'});\n                  }else{\n                    this.$Message.error(res.data.msg);\n                  }\n                })\n            }\n        }\n      },\n      mounted(){\n          this.getOrgan();\n      }\n    }\n</script>\n\n\n\n\n\n// WEBPACK FOOTER //\n// src/pages/source/inventory-new.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"new-inventory\"},[_c('Card',{staticClass:\"mb-15