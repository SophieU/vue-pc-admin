cancelOrder\" type=\"primary\" v-if=\"currentCon==='cancel'\">取消工单</Button>\n              <Button @click=\"startAfter\" type=\"primary\" v-if=\"currentCon==='after'\">发起售后</Button>\n            </div>\n          </div>\n\n        </div>\n      </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"order-control\",\n      data(){\n        return {\n          currentCon:'', //cancel:取消工单，after:发起今后\n          id:'',\n          reasonLists:[],\n          cancelReason:'',\n        }\n      },\n      methods:{\n        demandChange(value){\n          console.log(value);\n          switch(value){\n            case 'cancel':\n              this.currentCon='cancel';\n              break;\n            case 'after':\n              this.currentCon='after';\n              break;\n            default:\n              this.currentCon='';\n          }\n        },\n        cancelOrder(){\n          let params = {\n            reasonId:this.cancelReason\n          };\n          this.$http.post(`/repair/order/cancel?id=${this.id}&reasonId=${this.cancelReason}`,{\n            ...params\n          }).then(res=>{\n              if(res.data.code===0){\n                this.$Message.success('取消成功');\n                this.$router.push({name:'order-filter'});\n              }else{\n                this.$Message.error('取消失败，'+res.data.msg);\n              }\n            })\n        },\n        startAfter(){\n          this.$http.post(`/repair/order/after/sale/add?id=${this.id}`)\n            .then(res=>{\n              if(res.data.code===0){\n                this.$Message.success('售后发起成功');\n                this.$router.push({name:'order-filter'});\n              }else{\n                this.$Message.error('发起失败');\n              }\n            })\n        },\n        getCancelReason(){\n          this.$http.post(`/repair/order/cancel/reason/list`)\n            .then(res=>{\n              if(res.data.code===0){\n                this.reasonLists=res.data.data;\n              }else{\n                console.log('取消原因获取失败：'+res.data.msg);\n              }\n            })\n        }\n      },\n      mounted(){\n          let id = this.$route.query.id;\n          this.id=id;\n          this.getCancelReason();\n      }\n    }\n</script>\n\n\n\n\n// WEBPACK FOOTER //\n// src/pages/order/order-com/order-control.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"order-control\"},[_c('div',{staticClass:\"card\"},[_c('div',{staticClass:\"card-body\"},[_c('div',{staticClass:\"control-flex\"},[_c('div',{staticClass:\"label-title\"},[_vm._v(\"用户述求：\")]),_vm._v(\" \"),_c('div',{staticClass:\"select-wrap\"},[_c('Select',{staticClass:\"mb-15\",staticStyle:{\"width\":\"200px\",\"display\":\"block\"},on:{\"on-change\":_vm.demandChange}},[_c('Option',{attrs:{\"value\":\"cancel\"}},[_vm._v(\"取消订单\")]),_vm._v(\" \"),_c('Option',{attrs:{\"value\":\"after\"}},[_vm._v(\"发起售后\")])],1),_vm._v(\" \"),(_vm.currentCon==='cancel')?_c('Select',{staticStyle:{\"width\":\"200px\"},attrs:{\"placeholder\":\"请选择取消原因\"},model:{value:(_vm.cancelReason),callback:function ($$v) {_vm.cancelReason=$$v},expression:\"cancelReason\"}},_vm._l((_vm.reasonLists),function(reason){return _c('Option',{key:reason.id,attrs:{\"value\":reason.id}},[_vm._v(_vm._s(reason.reasonName))])})):_vm._e()],1),_vm._v(\" \"),_c('div',{staticClass:\"btn-wrap\"},[(_vm.currentCon==='cancel')?_c('Button',{attrs:{\"type\":\"primary\"},on:{\"click\":_vm.cancelOrder}},[_vm._v(\"取消工单\")]):_vm._e(),_vm._v(\" \"),(_vm.currentCon==='after')?_c('Button',{attrs:{\"type\":\"primary\"},on:{\"click\":_vm.startAfter}},[_vm._v(\"发起售后\")]):_vm._e()],1)])])])])}\nvar staticRenderFns = []\nvar esExports = { render: render, staticRenderFns: staticRenderFns }\nexport default esExports\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./node_modules/vue-loader/lib/template-compiler?{\"id\":\"data-v-4836c264\",\"hasScoped\":true,\"transformToRequ