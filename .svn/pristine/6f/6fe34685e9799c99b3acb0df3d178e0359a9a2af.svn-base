/th>\n          <th>创建时间</th>\n          <th>盘点盈亏</th>\n          <th>盈亏成本</th>\n          <th>操作人</th>\n        </tr>\n        </thead>\n        <tbody>\n        <tr>\n          <td>{{detail.departmentName}}</td>\n          <td>{{detail.orderSn}}</td>\n          <td>{{detail.createTime}}</td>\n          <td>{{detail.diffNum}}</td>\n          <td>{{detail.amount}}</td>\n          <td>{{detail.creator}}</td>\n        </tr>\n        </tbody>\n      </table>\n      <table class=\"native-table\">\n        <thead>\n        <tr>\n          <th>辅材名称</th>\n          <th>报修分类</th>\n          <th>型号</th>\n          <th>单位</th>\n          <th>成本价</th>\n          <th>系统数量</th>\n          <th>盘点数</th>\n          <th>盈亏数</th>\n          <th>盈亏成本</th>\n        </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(item,index) in detail.detailList\" :key=\"index\">\n            <td>{{item.materialName}}</td>\n            <td>{{item.repairCategoryName}}</td>\n            <td>{{item.materialSpec}}</td>\n            <td>{{item.unit}}</td>\n            <td>{{item.price}}</td>\n            <td>{{item.num}}</td>\n            <td>{{item.checkedNum}}</td>\n            <td>{{item.diffNum}}</td>\n            <td>{{item.amount}}</td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </Card>\n\n</template>\n\n<script>\n    export default {\n        name: \"inventory-detail\",\n      data(){\n        return {\n          detail:null\n        }\n      },\n      methods:{\n          getDetail(id){\n            this.$http.get(`/repair/material/check/order/info?id=${id}`)\n              .then(res=>{\n                if(res.data.code===0){\n                  this.detail=res.data.data;\n                }else{\n                  console.log('盘点详情：'+res.data.msg);\n                }\n              })\n          }\n      },\n      mounted(){\n          let id = this.$route.params.id;\n          this.getDetail(id);\n      }\n    }\n</script>\n\n<style scoped>\n\n</style>\n\n\n\n/