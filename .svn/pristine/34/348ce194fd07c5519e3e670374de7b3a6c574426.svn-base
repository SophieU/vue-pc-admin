             <!--<Option value=\"after\">发起售后</Option>-->\n              </Select>\n              <Select v-model=\"cancelReason\" style=\"width: 200px;\" v-if=\"currentCon==='cancel'\" placeholder=\"请选择取消原因\">\n                <Option v-for=\"reason in reasonLists\" :value=\"reason.id\" :key=\"reason.id\">{{reason.reasonName}}</Option>\n              </Select>\n            </div>\n            <div class=\"btn-wrap\">\n              <Button @click=\"cancelOrder\" type=\"primary\" v-if=\"currentCon==='cancel'\">取消工单</Button>\n              <Button @click=\"startAfter\" type=\"primary\" v-if=\"currentCon==='after'\">发起售后</Button>\n            </div>\n          </div>\n\n        </div>\n      </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"order-control\",\n      data(){\n        return {\n          currentCon:'', //cancel:取消工单，after:发起今后\n          id:'',\n          reasonLists:[],\n          cancelReason:'',\n          selectLists:[], //下拉选项\n        }\n      },\n      methods:{\n        demandChange(value){\n          console.log(value);\n          switch(value){\n            case 'cancel':\n              this.currentCon='cancel';\n              break;\n            case 'after':\n              this.currentCon='after';\n              break;\n            default:\n              this.currentCon='';\n          }\n        },\n        cancelOrder(){\n          let params = {\n            reasonId:this.cancelReason\n          };\n          this.$http.post(`/repair/order/cancel?id=${this.id}&reasonId=${this.cancelReason}`,{\n            ...params\n          }).then(res=>{\n              if(res.data.code===0){\n                this.$Message.success('取消成功');\n                this.$router.push({name:'order_filter'});\n              }else{\n                this.$Message.error('取消失败，'+res.data.msg);\n              }\n            })\n        },\n        startAfter(){\n          this.$http.post(`/repair/order/after/sale/add?id=${this.id}`)\n            .then(res=>{\n              if(res.data.code===0){\n                this.$Message.success('售后发起成功');\n                this.$router.push({name:'order_filter'});\n              }else{\n                this.$Message.error('发起失败:'+res.data.msg);\n              }\n            })\n        },\n        getCancelReason(){\n          this.$http.post(`/repair/order/cancel/reason/list`)\n            .then(res=>{\n              if(res.data.code===0){\n                this.reasonLists=res.data.data;\n              }else{\n                console.log('取消原因获取失败：'+res.data.msg);\n              }\n            })\n        },\n\n        setControl(){\n          let state = this.state;\n          let cancleAble=[\"待上门\",\"待分配\"];\n          let serviceAble=[\"已完成\"];\n          let isInWarranty=this.$route.query.isWarranty;\n          let selectLists=[];\n          if(cancleAble.indexOf(state)>-1){\n            selectLists.push({name:'取消订单',value:'cancel'})\n          }\n        // &&isInWarranty==='是'\n          if(serviceAble.indexOf(state)>-1){\n            selectLists.push({name:'发起售后',value:'after'})\n          }\n          this.selectLists=selectLists;\n        }\n      },\n      mounted(){\n          let id = this.$route.query.id;\n          this.state=this.$route.query.state;\n          this.id=id;\n          this.getCancelReason();\n          this.setControl();\n      }\n    }\n</script>\n\n\n\n\n// WEBPACK FOOTER //\n// src/pages/order/order-com/order-control.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"order-control\"},[_c('div',{staticClass:\"card\"},[_c('div',{staticClass:\"card-body\"},[_c('div',{staticClass:\"control-flex\"},[_c('div',{staticClass:\"label-title\"},[_vm._v(\"用户述求：\")]),_vm._v(\" \"),_c('div',{staticClass:\"select-wrap\"},[_c('Select',{staticClass:\"mb-15\",staticStyle:{\"width\":\"200px\",\"display\":\"block\"},on:{\"on-change\":_vm.demandChange}},_vm._l((_vm.selectList